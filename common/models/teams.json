{
  "name": "teams",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "ei_creator": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "number",
      "default": 0
    },
    "participants": {
      "type": "number",
      "default": 0
    },
    "logo": {
      "type": "string"
    },
    "chat": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "participants": {
      "type": "belongsTo",
      "model": "participants"
    },
    "users": {
      "type": "hasMany",
      "model": "users",
      "foreignKey": "team_id",
      "through": "teams_players_link",
      "keyThrough": "user_id"
    }
  },
  "acls": [],
  "methods": {}
}
